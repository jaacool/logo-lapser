{
  "functions": {
    "api/*.js": {
      "maxDuration": 60
    }
  },
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key"
  },
  "build": {
    "env": {
      "FORCE_DEPLOY": "true",
      "BUILD_VERSION": "v5.3-FINAL"
    }
  },
  "installCommand": "npm install --force",
  "framework": "vite",
  "outputDirectory": "dist",
  "deployHooks": [
    {
      "id": "force-deploy",
      "url": "https://api.vercel.com/v1/integrations/deploy/prj_q49KmHH12RGhHmKAHaQvvzuZ3G9U/1BgZsFFsvJ"
    }
  ]
}
